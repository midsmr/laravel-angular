{
    "name": "midsmr/laravel-angular",
    "description": "Bootstrap Angular Template For Laravel",
    "keywords": [
        "midsmr",
        "laravel",
        "laravel-angular"
    ],
    "homepage": "https://github.com/midsmr/laravel-angular",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerry",
            "email": "midsmr@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Midsmr\\LaravelAngular\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Midsmr\\LaravelAngular\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Midsmr\\LaravelAngular\\LaravelAngularServiceProvider"
            ],
            "aliases": {
                "LaravelAngular": "Midsmr\\LaravelAngular\\Facades\\LaravelAngular"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
